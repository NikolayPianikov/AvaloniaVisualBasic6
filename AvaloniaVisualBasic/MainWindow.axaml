<ClassicWindow xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:avaloniaVisualBasic="clr-namespace:AvaloniaVisualBasic"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="AvaloniaVisualBasic.MainWindow"
        Icon="/vb6icon.ico"
        ClosingBehavior="OwnerAndChildWindows"
        DataContext="{StaticResource Composition}"
        Design.DataContext="{d:DesignInstance avaloniaVisualBasic:Composition}"
        x:DataType="avaloniaVisualBasic:Composition"
        Title="{CompiledBinding Root.Title}">
    <avaloniaVisualBasic:MainView x:Name="MainView" />
</ClassicWindow>
